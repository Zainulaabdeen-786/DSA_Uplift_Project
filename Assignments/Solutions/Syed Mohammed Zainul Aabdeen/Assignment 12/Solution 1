#include <bits/stdc++.h>
using namespace std;
int main(){
    int n,flag=0;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
        cin>>a[i];
    sort(a,a+n);
    for(int i=0;i<n-1;i++){
        if(a[i]>0 && a[i+1]!=a[i]+1){
            flag=1;
            cout<<a[i]+1<<endl;
            break;
        }
    }
    if(flag==0)
        cout<<1<<endl;
    return 0;
}
